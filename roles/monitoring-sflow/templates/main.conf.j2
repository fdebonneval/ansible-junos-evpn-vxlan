/* This block of configuration has been generated by the role monitoring-sflow for Ansible */
{% if sflow is defined %}
protocols {
    sflow {
        polling-interval 30;
        adaptive-sample-rate 9500;
        sample-rate {
            ingress 15000;
            egress 15000;
        }
        source-ip {{loopback_ip}};
        collector {{sflow.collector}} {
            udp-port {{sflow.collector_port}};
        }
{% for interface in sflow.interfaces %}
                    interfaces {{interface.name}};
{% endfor %}
    }
}	
{% endif %}
